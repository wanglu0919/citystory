<?xml version="1.0" encoding="UTF-8"?> 
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.feiyang.entity.User">
	<insert id="addUser" parameterType="User">
		INSERT INTO U_USER  VALUES(#{id},#{name},#{age},#{cityId},#{cityName},#{provinceId},#{provinceName},#{password},#{accessToken},#{tokenUpdatetTime},#{registTime})
	</insert>

<select id="findUserByName" parameterType="string" resultType="User">
	SELECT name,password from U_USER where name=#{name}
</select>


</mapper>